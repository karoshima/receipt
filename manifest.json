{
    "manifest_version": 2,
    "name": "receipt",
    "version": "0.1",

    "description": "popup for excel",

    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },

    "content_scripts": [
        {
            "matches": [ "https://*.mizuhobank.co.jp/*" ],
            "js": [ "mizuhobank.js" ]
        },
        {
            "matches": [ "https://*.jp-bank.japanpost.jp/*" ],
            "js": [ "yucho.js" ]
        }
    ]
}
