{
    "manifest_version": 2,
    "name": "receipt",
    "version": "0.1",

    "description": "popup for excel",

    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },

    "content_scripts": [
        {
            "matches": [ "https://*.mizuhobank.co.jp/*" ],
            "js": [ "sheet.js", "mizuhobank.js" ]
        },
        {
            "matches": [ "https://*.jp-bank.japanpost.jp/*" ],
            "js": [ "sheet.js", "yucho.js" ]
        },
        {
            "matches": [ "https://netanswerplus.saisoncard.co.jp/WebPc/*" ],
            "js": [ "sheet.js", "saison.js" ]
        },
        {
            "matches": [ "https://atunet.uccard.co.jp/*" ],
            "js": [ "sheet.js", "uccard.js"]
        },
        {
            "matches": [ "https://www.aeon.co.jp/app/" ],
            "js": [ "sheet.js", "aeon.js" ]
        }
    ]
}
